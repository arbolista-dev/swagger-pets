    "test:all": "NODE_ENV=test MYSQL_DATABASE=icracked_new_test MYSQL_HOST=127.0.0.1 mocha test",
    "test:integration": "NODE_ENV=test MYSQL_DATABASE=icracked_new_test MYSQL_HOST=127.0.0.1 mocha test/helper.js test/integration"
